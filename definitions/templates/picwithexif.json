{
    "type": "template",
    "js": ["https://cdn.jsdelivr.net/npm/exif-js"],
    "name": "picwithexif",
    "close": true,
    "content": [
        {
            "name": "div",
            "attr": {
                "style": {
                    "width": "$_pwexif_template_width$",
                    "height": "$_pwexif_template_height$"
                }
            },
            "content": [
                {
                    "name": "div",
                    "content": [
                        {
                            "name": "pic",
                            "attr": {
                                "style": {
                                    "width": "$_pwexif_template_picwidth$",
                                    "height": "$_pwexif_template_picheight$",
                                    "max-width": "100%"
                                }
                            },
                            "vars": {
                                "src": "$_pwexif_template_src$",
                                "id": ["$_pwexif_template_id$", "_pic"]
                            }
                        }
                    ]
                },
                {
                    "name": "h5",
                    "content": ["$_pwexif_template_title$"]
                },
                {
                    "name": "h6",
                    "vars": {
                        "src": "$_pwexif_template_src$",
                        "id": ["$_pwexif_template_id$", "_exif"]
                    },
                    "content": ""
                    
                },
                {
                    "name": "p",
                    "content": ["$_pwexif_template_description$"]
                },
                {
                    "name": "script",
                    "content": [
                        "var ", "$_jc_template_json_id$", "=", "$_jc_template_json$", ";", 
                        "$('#", "$_pwexif_template_id$", "_exif').html('", "EXIF.getTag(this, \"Make\")", " EXIF.getTag(this, \"Model\")", " EXIF.getTag(this, \"FocalLength\")", " EXIF.getTag(this, \"ShutterSpeedValue\")", " EXIF.getTag(this, \"FNumber\")", " EXIF.getTag(this, \"ApertureValue\")", "');"
                    ]
                }
            ]
        }
    ],
    "vars": {
        "_pwexif_template_id": "",
        "_pwexif_template_width": "",
        "_pwexif_template_height": "",
        "_pwexif_template_picwidth": "",
        "_pwexif_template_picheight": "",
        "_pwexif_template_src": "/img/test.png",
        "_pwexif_template_title": "Title",
        "_pwexif_template_description": "This is a description."
    }
}