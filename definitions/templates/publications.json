{
    "name": "publications",
    "type": "template",
    "js": ["./js/publications.js"],
    "content": [
        {
            "name": "div",
            "attr": {
                "style": {
                    "text-align": "$_pub_template_align$"
                }
            },
            "content": [
                ["$_pub_template_authors$", 
                ".<b> ", 
                "$_pub_template_title$", 
                "</b>. <i>", 
                "$_pub_template_pubon$", 
                "</i> ", 
                "$_pub_template_year$", 
                ".", 
                "&nbsp;<a href=\"",
                "$_pub_template_link$",
                "\" target=\"_blank\" onclick=\"if ($(this.attr('href') == '' || $(this.attr('href') == undefined)){alert('DOI is not yet available.');event.preventDefault();return false;}\"><h5 style=\"display: inline;\">doi</h5></a>", 
                "&nbsp;<a style=\"cursor: pointer\" onclick=\"copyBibTeX(this)\" data-bib=\"",
                "$_pub_template_bibtex$",
                "\"><h5 style=\"display: inline;\">bib</h5></a>"]
            ]
        }
    ],
    "vars": {
        "_pub_template_authors": "Homer Simpson, Seymour Skinner, Apu Nahasapeemapetilon, and Barney Gumble",
        "_pub_template_title": "Baby on Board",
        "_pub_template_pubon": "Bigger than Jesus",
        "_pub_template_year": "1990",
        "_pub_template_link": "",
        "_pub_template_bibtex": "",
        "_pub_template_align": "left"
    }
}