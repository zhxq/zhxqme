{
    "name": "titlewpara",
    "type": "template",
    "content": [
        ["<div class=\"container-fluid\" style=\"max-width: ", "$_twp_template_maxwidth$","; margin: 0 auto; text-align: ", "$_twp_template_align$",";\">"],
        {
            "name": "div",
            "attr": {
                "style": {
                    "text-align": "$_twp_template_align$",
                    "max-width": "$_twp_template_maxwidth$",
                    "display": "inline-block"
                }
            },
            "content": [
                {
                    "name": "$_twp_template_titletype$",
                    "content": ["$_twp_template_title$"]
                },
                {
                    "name": "cpara",
                    "vars": {
                        "_cpara_template_content": ["$_twp_template_paragraph$"],
                        "_cpara_template_maxwidth": "$_twp_template_maxwidth$"
                    }
                }
            ]
        },
        ["</div>"]
    ],
    "vars": {
        "_twp_template_title": "Title",
        "_twp_template_paragraph": "Some content in Paragraph",
        "_twp_template_maxwidth": "800px",
        "_twp_template_align": "center",
        "_twp_template_titletype": "h3"
    }
}